# AI Provider Configuration
AI_PROVIDER=openrouter
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=openai/gpt-4o

# Instagram Bot Configuration
IGusername=your_instagram_username
IGpassword=your_instagram_password

# Twitter/X Bot Configuration (Optional)
Xusername=your_twitter_username
Xpassword=your_twitter_password

# Twitter API Credentials (Optional)
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_SECRET=your_twitter_access_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token

# Gemini API Keys (Optional - for Gemini AI provider)
# Add as many as you need, numbered sequentially
GEMINI_API_KEY_1=your_gemini_api_key_1_here
GEMINI_API_KEY_2=your_gemini_api_key_2_here
GEMINI_API_KEY_3=your_gemini_api_key_3_here
# Add more GEMINI_API_KEY_X as needed (up to 50)

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/instagram-ai-agent

# Security
SESSION_SECRET=change_this_to_a_random_secret_key
JWT_SECRET=change_this_to_a_random_jwt_secret

# Webhook Security (Optional)
WEBHOOK_API_KEY=your_webhook_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development
